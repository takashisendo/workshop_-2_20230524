   "source": [
    "import uuid\n",
    "\n",
    "def generate_itn_did():\n",
    "    # Generate a random UUID\n",
    "    did_uuid = str(uuid.uuid4())\n",
    "\n",
    "    # Build the ITN DID string\n",
    "    did = \"did:itn:\" + did_uuid\n",
    "\n",
    "    return did\n",
    "\n",
    "# Generate an ITN DID\n",
    "itn_did = generate_itn_did()\n",
    "print(\"Generated ITN DID:\", itn_did)\n"
   ]
